<form class="form-signin" action="/media/" method="POST" enctype="multipart/form-data">>
  <h2 class="form-signin-heading">upload file</h2>

  <label for="inputFile" class="sr-only">file</label>
  <input type="file" id="inputFile" name="filefield" class="form-control" placeholder="file" required autofocus>

    <input type="text" name="textfield" value="file" hidden><br />

  <br/>
  
  <button class="btn btn-lg btn-default btn-block" type="submit">upload</button>
</form>
